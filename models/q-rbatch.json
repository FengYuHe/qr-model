{
  "name": "QRbatch",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "ShortId": {},
    "TimeStamp": {
      "createdAt": "created_at",
      "updatedAt": "updated_at",
      "silenceWarnings": true
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "task_id": {
      "type": "string"
    },
    "task_name": {
      "type": "string"
    },
    "no": {
      "type": "string"
    },
    "product_id": {
      "type": "string"
    },
    "product_name": {
      "type": "string"
    },
    "win_rate_id": {
      "type": "string"
    },
    "win_rate_name": {
      "type": "string"
    },
    "start_code": {
      "type": "string"
    },
    "end_code": {
      "type": "string"
    },
    "count_code": {
      "type": "number"
    },
    "status": {
      "type": "number",
      "default": 0
    },
    "created_at": {
      "type": "date"
    },
    "updated_at": {
      "type": "date"
    },
    "remark": {
      "type": "string"
    }
  },
  "scope": {
  },
  "validations": [],
  "relations": {
    "task": {
      "type": "belongsTo",
      "model": "QRTask",
      "foreignKey": "task_id"
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "product_id",
      "primaryKey": "item_id"
    },
    "qrWinRate": {
      "type": "belongsTo",
      "model": "QRWinRate",
      "foreignKey": "win_rate_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "terminate"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "setWinRate"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "activation"
    }
  ],
  "methods": {}
}
